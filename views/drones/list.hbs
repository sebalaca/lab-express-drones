
<div>

  <h2>Available drones</h2>

  {{#each response}}

    <h5>{{name}}</h5>
    <p>{{propellers}}</p>
    <p>{{maxSpeed}}</p>
    <form action="/drones/{{_id}}/delete" method="POST"> 
      <button>Delete</button>
    </form>
        <form action="/drones/{{_id}}/edit" method="POST"> 
      <button>Edit</button>
    </form>
  {{/each}}

</div>